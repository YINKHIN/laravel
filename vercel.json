{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_KEY": "base64:your-generated-key-here",
    "APP_DEBUG": "false",
    "DB_CONNECTION": "mysql",
    "DB_HOST": "127.0.0.1",
    "DB_PORT": "3306",
    "DB_DATABASE": "stock_inout",
    "DB_USERNAME": "vercel_user",
    "DB_PASSWORD": "secure_password"
  }
}